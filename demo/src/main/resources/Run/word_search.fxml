<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.GridPane?>

<AnchorPane prefHeight="510.0" prefWidth="769.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Run.WordSearchController">
   <children>
      <GridPane fx:id="gridPane" hgap="5" layoutX="50.0" layoutY="50.0" vgap="5">
      </GridPane>
      <Button fx:id="submitButton" layoutX="504.0" layoutY="98.0" mnemonicParsing="false" onAction="#handleSubmitButtonClick" prefHeight="32.0" prefWidth="109.0" styleClass="game-button" text="Submit" />
      <Label fx:id="foundWordsLabel" alignment="TOP_LEFT" layoutX="504.0" layoutY="136.0" prefHeight="275.0" prefWidth="225.0" styleClass="meaning-input" text="Đã thấy từ: " />
      <Button fx:id="deleteWordButton" layoutX="612.0" layoutY="98.0" mnemonicParsing="false" onAction="#restartWord" prefHeight="32.0" prefWidth="117.0" styleClass="game-button" text="Xóa" />
      <Button fx:id="highScoreButton" layoutX="504.0" layoutY="470.0" mnemonicParsing="false" onAction="#showHighScores" text="Điểm cao" />
      <Button fx:id="suggestWordButton" layoutX="618.0" layoutY="427.0" mnemonicParsing="false" onAction="#showSuggest" styleClass="add-button" text="Gợi ý" />
      <Button fx:id="restartGameButton" layoutX="151.0" layoutY="8.0" mnemonicParsing="false" onAction="#restartGame" prefWidth="163.0" styleClass="add-button" text="Tạo lại bảng" />
      <Label fx:id="scoreLabel" layoutX="504.0" layoutY="423.0" prefHeight="33.0" prefWidth="81.0" styleClass="meaning-input" text="Điểm: 0" />
   </children>
</AnchorPane>
